<launch>
  <!-- spawn stopped position-resolved controllers -->
  <rosparam command="load" file="$(find pr2_controller_configuration)/pr2_arm_controllers.yaml" />
  <node name="pos_arm_controller_spawner" pkg="pr2_controller_manager" type="spawner"
      output="screen" args="--stopped r_arm_controller l_arm_controller"/>

  <!-- spawn stopped velocity-resolved controllers -->
  <include file="$(find iai_robot_mechanism_controllers)/launch/pr2_multi_vel_load.launch"/>

  <!-- constraint arm controllers -->
  <include file="$(find fccl_nodes)/launch/pr2_fccl_arm_controllers.launch"/>
</launch>
